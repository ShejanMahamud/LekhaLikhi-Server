{
  "version": 2,
  "builds": [
    {
      "src": "./src/app/app.ts",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb",
        "timeout": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/app/app.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
    }
  ]
}
